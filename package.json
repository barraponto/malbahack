{
  "name": "malbahack",
  "version": "0.1.0",
  "description": "A historia da *rte",
  "main": "src/index.js",
  "scripts": {
    "build": "run-s build:*",
    "watch": "run-p watch:*",
    "deploy": "run-s build deploy:*",
    "build:entries": "m2j --content --outfile src/data/entries.json data/entries/*.md",
    "watch:entries": "chokidar data/entries/ -c \"npm run build:entries\"",
    "build:about": "m2j --content --outfile src/data/about.json data/about/*.md",
    "watch:about": "chokidar data/about/ -c \"npm run build:about\"",
    "build:neutrino": "neutrino build",
    "watch:neutrino": "neutrino start",
    "deploy:netlify": "netlify deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "neutrino": {
    "use": [
      "neutrino-preset-eslint-google",
      "neutrino-preset-stylelint-standard",
      "neutrino-preset-web",
      "neutrino-preset-cssnext",
      "neutrino-preset-vue"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barraponto/webpack-config.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Capi Etheriel <barraponto@gmail.com> (https://barraponto.blog.br)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/barraponto/webpack-config/issues"
  },
  "homepage": "https://github.com/barraponto/webpack-config#readme",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "netlify-cli": "^1.2.2",
    "neutrino": "^5.7.0",
    "neutrino-preset-cssnext": "^1.1.1",
    "neutrino-preset-eslint-google": "^2.0.1",
    "neutrino-preset-stylelint-standard": "^2.0.0",
    "neutrino-preset-vue": "^2.1.0",
    "neutrino-preset-web": "^5.7.0",
    "npm-run-all": "^4.0.2",
    "stylelint": "^7.10.1"
  },
  "dependencies": {
    "markdown": "^0.5.0",
    "markdown-to-json": "^0.5.1",
    "normalize.css": "^6.0.0",
    "velocity-animate": "^1.5.0",
    "vue": "^2.2.6",
    "vue-router": "^2.3.1"
  }
}
